{
    "name": "dev-giri/pin-india",
    "description": "Laravel package for managing and querying Indian post office data",
    "keywords": ["laravel", "post office", "pincode", "india", "postal", "address", "location", "geolocation", "indian postoffice", "pincode verification", "pincode validation"],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Dev Giri",
        "email": "dev.giridhari@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "PinIndia\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "PinIndia\\PinIndiaServiceProvider"
        ]
      }
    },
    "require": {
      "php": "^8.0",
      "illuminate/support": "^9|^10|^11|^12"
    },
    "require-dev": {
      "orchestra/testbench": "^7.0|^8.0",
      "phpunit/phpunit": "^9.0|^10.0"
    },
    "scripts": {
      "test": "vendor/bin/phpunit",
      "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
